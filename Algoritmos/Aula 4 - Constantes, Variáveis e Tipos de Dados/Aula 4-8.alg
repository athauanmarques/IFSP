algoritmo "semnome"
// Função :
// Autor :
// Data : 30/06/2013
// Seção de Declarações 
var
A, B, soma, produto, quociente: real

inicio
escreval ("Digite  o valor A: ")
leia (A)
escreval ("Digite o valor B: ")
leia (B)

soma <- A + B
produto <- A * B
quociente <- A / B

ESCREVAl ("resultados ")
escreva ("som: ", soma," - produto: ", produto," - quociente: ", quociente)
fimalgoritmo
