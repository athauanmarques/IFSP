algoritmo "semnome"
// Função :
// Autor :
// Data : 26/03/2013
// Seção de Declarações 
var
p, ex, vm: inteiro

inicio
// Seção de Comandos
escreval ("escreva o valor do peso do peixe: ")
leia (p)

se ( p > 50) entao
ex <- peso - 50
vm <- ex * 4
fimse

escreval ("peso: ", p)
escreval ("excesso: ", ex)
escreval ("valor da multa: ", vm)
 
fimalgoritmo
