algoritmo "semnome"
// Função :
// Autor :
// Data : 19/03/2013
// Seção de Declarações 
var
num, P, I: Inteiro

inicio
// Seção de Comandos
   Escreva ("Informe um numero: ")
   Leia (num)
   
   Se (num % 2 <> 0) Entao
      I <- num
   Senao
      P <- num
   FimSe
   
   Escreval ("Numero par= ",P)
   Escreva ("Numero ímpar= ",I)
   
fimalgoritmo
