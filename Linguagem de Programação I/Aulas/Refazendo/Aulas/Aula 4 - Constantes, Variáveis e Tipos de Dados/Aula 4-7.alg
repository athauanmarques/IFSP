algoritmo "semnome"
// Função :
// Autor :
// Data : 30/06/2013
// Seção de Declarações 
var
nota1, nota2, media: real

inicio
escreva ("Digite a 1ª nota: ")
leia (nota1)
escreva ("Digite a 2ª nota: ")
leia (nota1)

media <- (nota1*3 + nota2*7)/10

escreval ("A media da nota é : ", media)
fimalgoritmo
